!> Test for reading an hdf grid with no pmm assigned. D1_E1_M1 = One Domain, One Element, Linear Mapping
@Test
subroutine testIO__read_pmm_hdf__nopmm__D1_E1_M1() 
    use pfunit_mod
    use type_chidg,             only: chidg_t
    use mod_kinds,              only: rk,ik
    use mod_constants,          only: ZERO, ONE, IO_DESTINATION, NO_PMM_ASSIGNED
    use mod_test_utilities,     only: create_mesh_file
    use mod_file_utilities,     only: delete_file
    use mod_grid,               only: compute_element_mappings
    implicit none
    
    type(chidg_t)                   :: chidg
    character(100), parameter       :: filename = 'D1_E1_M1.h5'
    integer,        parameter       :: ndomains = 1
    integer(ik),    parameter       :: spacedim = 3
    integer(ik),    parameter       :: nterms_c = 8
    integer,        parameter       :: nelem    = 1
    real(rk)                        :: x(8), y(8), z(8)
    real(rk)                        :: tol
    
    tol = 1.e-14_rk
    IO_DESTINATION = 'file'


    ! Initialize element mapping
    call chidg%start_up('core')
    

    !
    ! Create mesh file
    !
    call create_mesh_file("D1 NxNxN", filename,nelem_xi=1,nelem_eta=1,nelem_zeta=1)

    !
    ! Read grid and add domain to chidg
    !
    call chidg%read_grid(filename, spacedim)


    call chidg%set('Solution Order', integer_input=2)
    call chidg%init('domains')

    !  Test nelem
    @assertEqual(NO_PMM_ASSIGNED,chidg%data%mesh(1)%elems(1)%pmm_ID)
    @assertEqual(NO_PMM_ASSIGNED,chidg%data%mesh(1)%faces(1,1)%pmm_ID)
    @assertEqual(NO_PMM_ASSIGNED,chidg%data%mesh(1)%faces(1,2)%pmm_ID)
    @assertEqual(NO_PMM_ASSIGNED,chidg%data%mesh(1)%faces(1,3)%pmm_ID)
    @assertEqual(NO_PMM_ASSIGNED,chidg%data%mesh(1)%faces(1,4)%pmm_ID)
    @assertEqual(NO_PMM_ASSIGNED,chidg%data%mesh(1)%faces(1,5)%pmm_ID)
    @assertEqual(NO_PMM_ASSIGNED,chidg%data%mesh(1)%faces(1,6)%pmm_ID)
    
    
    !
    ! Close ChiDG
    !
    call chidg%shut_down('core')

    call delete_file(filename)

end subroutine testIO__read_pmm_hdf__nopmm__D1_E1_M1
!******************************************************************************************


!> Test for reading an hdf grid with static pmm assigned. D1_E1_M1 = One Domain, One Element, Linear Mapping
