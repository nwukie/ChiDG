    !
    ! Read grid and add domain to chidg
    !
    call chidg%read_grid(filename, spacedim)

    call chidg%read_prescribedmeshmotions(filename)

    call chidg%set('Solution Order', integer_input=2)
    call chidg%init('domains')

    !  Test nelem
    @assertEqual('sinusoidal', chidg%data%pmm(1)%pmmf_name)
    @assertEqual('sin_pmm', chidg%data%pmm(1)%get_name())
    @assertEqual(1.5_rk, chidg%data%pmm(1)%pmmf%get_option_value('L_X'))
    @assertEqual(1, chidg%data%pmm(1)%pmm_ID)
    @assertEqual(1,chidg%data%mesh(1)%elems(1)%pmm_ID)
    @assertEqual(1,chidg%data%mesh(1)%faces(1,1)%pmm_ID)
    @assertEqual(1,chidg%data%mesh(1)%faces(1,2)%pmm_ID)
    @assertEqual(1,chidg%data%mesh(1)%faces(1,3)%pmm_ID)
    @assertEqual(1,chidg%data%mesh(1)%faces(1,4)%pmm_ID)
    @assertEqual(1,chidg%data%mesh(1)%faces(1,5)%pmm_ID)
    @assertEqual(1,chidg%data%mesh(1)%faces(1,6)%pmm_ID)
    
    
    !
    ! Close ChiDG
    !
    call chidg%shut_down('core')

    call delete_file(filename)

end subroutine testIO__read_pmm_hdf__sinpmm__D1_E1_M1
