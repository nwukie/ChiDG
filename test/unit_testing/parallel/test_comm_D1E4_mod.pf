                ! DIAG
                imat = A%dom(1)%lblks(1,1)%loc(1,2)
                A%dom(1)%lblks(1,1)%data_(imat)%mat = TWO

                ! XI_MAX
                imat = A%dom(1)%lblks(1,1)%loc(1,3)
                A%dom(1)%lblks(1,1)%data_(imat)%mat = SIX

                ! X-Vector
                x%dom(1)%vecs(1)%vec = 12._rk

                
            else if (IRANK == 2) then
                !
